{
    "name": "chordsheets",
    "version": "1.5.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:minor": "npm version minor && npm run pages:update",
        "build:major": "npm version major && npm run pages:update",
        "build:patch": "npm version patch && npm run pages:update",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "pages:update": "npm run build && git add -A && git commit -m \"new: build update\" && git push"
    },
    "dependencies": {
        "jspdf": "^4.1.0",
        "lodash": "^4.17.23",
        "pinia": "^3.0.4",
        "svg2pdf.js": "^2.7.0",
        "vue": "^3.5.28",
        "vue-router": "^5.0.2",
        "vuedraggable": "^4.1.0",
        "vite-plugin-pwa": "^1.2.0"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.15.0",
        "@tonaljs/chord-detect": "^4.9.1",
        "@types/node": "^25.2.3",
        "@vitejs/plugin-vue": "^6.0.4",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "dom-to-image": "^2.6.0",
        "eslint": "^9.39.2",
        "eslint-plugin-vue": "^10.8.0",
        "html2canvas": "^1.4.1",
        "midi-parser-js": "^4.0.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.8.1",
        "tonal": "^6.4.3",
        "tone": "^15.1.22",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vue-tsc": "^3.2.4"
    }
}